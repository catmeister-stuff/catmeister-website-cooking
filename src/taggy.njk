---
pagination:
  data: collections
  size: 1
  alias: tag
permalink: /{{ tag }}/
layout: base
eleventyComputed:
  title: "foods most {{ tag }}-y"
---

<h1>{{ tag }}</h1>

<ul>
{% set taglist = collections[ tag ]  %}
{% for post in taglist | reverse %}
  <li><a href="{{ post.url }}">{{ post.data.title }}</a></li>
{% endfor %}
</ul>